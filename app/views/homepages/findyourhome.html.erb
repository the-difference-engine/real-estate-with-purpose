<div class="holder">
  <div class="list-content home-search">
    <h2>PROPERTY SEARCH</h2>
    <div class="home-form">
      <div class="find-home">
        <%= form_tag '/properties', method: :post do %>

<!--           <div class="form-bedbath"> -->
            <%= text_field_tag :location, nil, placeholder: "Address, neighborhood, zip", style: "width: 200px" %>
      <!--     </div> -->
        <%= check_box_tag(:single) %>
        <%= label_tag(:single, "Single Family Home") %>
        <%= check_box_tag(:townhouse) %>
        <%= label_tag(:townhouse, "Townhouse") %>
        <br>
        <%= check_box_tag(:condo) %>
        <%= label_tag(:condo, "Condo") %>
        <%= check_box_tag(:multi) %>
        <%= label_tag(:multi, "Multi-unit") %>

        <div class="form-line">
          <div class="form-bedbath">
            <%= text_field_tag :price_min, nil, placeholder: "Price (min.)"%>
          </div>
          <div class="form-bedbath">
            <%= text_field_tag :price_max, nil, placeholder: "Price (max.)" %>
          </div>
        </div>
      </div>
      <div class="find-home">
        <div class="form-line">
          <div class="form-bedbath">
            <%= text_field_tag :beds_min, nil, placeholder: "Beds (min.)" %>
          </div>
          <div class="form-bedbath">
            <%= text_field_tag :beds_max, nil, placeholder: "Beds (max.)" %>
          </div>
        </div>
        <div class="form-line">
          <div class="form-bedbath">
            <%= text_field_tag :baths_min, nil, placeholder: "Baths (min.)"%>
          </div>
          <div class="form-bedbath">
            <%= text_field_tag :baths_max, nil, placeholder: "Baths (max.)" %>
          </div>
        </div>
        <%= submit_tag "Submit" %>
      </div>
      </div>
    <% end %>
  </div>

  <div class="list-content calc">
    <h2> Donation Calculator </h2>
    <h3> Total amount donated </h3>
        <span class="input-group-addon">Enter your budget: $</span>
        <input type="text" class="form-control" id="budget">
        <span class="input-group-addon">See your donation amount! (USD)</span>
        <input type="text" class="form-control" name="textbox" id="textbox" readonly="true">
  </div>
  </div>


